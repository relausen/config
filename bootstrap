#! /usr/bin/env bash

# Setup script for devpod

export XDG_CONFIG_HOME="$HOME"/.config

ln -s $HOME/dotfiles/.cargo .cargo
ln -s $HOME/dotfiles/.config .config
ln -s $HOME/dotfiles/.terminfo .terminfo
ln -s $HOME/dotfiles/.zshrc .zshrc
ln -s $HOME/dotfiles/.zfunc .zfunc
ln -s $HOME/dotfiles/.zprofile .zprofile
ln -s $HOME/dotfiles/.zshenv .zshenshenv
